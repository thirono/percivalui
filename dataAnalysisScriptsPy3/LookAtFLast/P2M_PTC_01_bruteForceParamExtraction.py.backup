    for iSet in range(NSets): 
        APy3_GENfuns.plot_2D_all(alldata_4DAr[i_light_avg,iSet,:,:], False, 'col','row','avg light {0}'.format(iSet), True); matplotlib.pyplot.show(block=True)
        APy3_GENfuns.dot()
