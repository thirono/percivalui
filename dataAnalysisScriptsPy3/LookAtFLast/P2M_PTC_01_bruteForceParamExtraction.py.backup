#            #matplotlib.pyplot.plot(numpy.log10(alldata_4DAr[i_light_avg,:,thisRow,thisCol]),numpy.log10(alldata_4DAr[i_light_std,:,thisRow,thisCol]), '--o', fillstyle='none')
